# AWS credentials (required for AWS CLI and SDKs)
AWS_ACCESS_KEY=<your-access-key>
AWS_SECRET_ACCESS_KEY=<your-secret-key>

# CloudFront domain and base URL
CF_DOMAIN=<your-cloudfront-domain>
BASE_URL=https://<your-cloudfront-domain>

# Start time for experiments (ISO format)
START_FROM_ISO="2025-09-02T16:00:00Z"
AWS_PROFILE=default
AWS_REGION=us-east-1
S3_LOGS_BUCKET=<ACCOUNT>-juice-ecs-logs-us-east-1
S3_WAF_BUCKET=<ACCOUNT>-juice-ecs-waf-logs-us-east-1
S3_FEATURE_PREFIX=feature-store/joined/
S3_MODELS_PREFIX=models/
WAF_WEBACL_ARN=arn:aws:wafv2:us-east-1:<ACCOUNT>:global/webacl/juice-ecs-web-acl/<UUID>
WAF_IPSET_NAME=adaptive-blocklist
WAF_IPSET_ID=<ipset-id>
LOOKBACK_HOURS=72
EST_LAT_MS=500 
THINK_MS=30 
VU_SAFETY_FACTOR=5 
VU_MAX_CAP=2000
WARM_MINS=5  
BREEZE_MINS=30  
COOL_MINS=5
WARM_RPS=3   
BREEZE_RPS=5   
COOL_RPS=3
ATT_START_OFFSET_MINS=15
ATT_RAMP_MINS=2  
ATT_PEAK_MINS=16  
ATT_COOL_MINS=2
ATT_TARGET_RPS=12
ATT_WARM_FRAC=0.5
ATT_COOL_FRAC=0.5
SURGE_RAMP_MINS=3  
SURGE_PEAK_MINS=30  
SURGE_COOL_MINS=7
SURGE_START_RPS=6  
SURGE_PEAK_RPS=12   
SURGE_END_RPS=6